<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.myeongsic.androidcodestore.MainActivity">

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/requestPermssion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Request Permssion" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/getPathFromGallery"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Get Path From Gallery" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/getAlertDailogActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Get AlertDailogActivity" />

    <android.support.v7.widget.AppCompatButton
        android:id="@+id/getIncomingService"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="GetIncomingService" />
</LinearLayout>
